.sub_agent_card
  border-radius 50px
  margin 30px 10px 0
  padding 24px 30px
  background #fff
  &:first-child
    margin-top 0
  &:last-child
    margin-bottom 20px
  .title
    color #333
    font-size 28px
    padding-bottom  22px
    border-bottom  1px solid #eee
    strong
      margin-left 20px
  .nav
    padding 35px 0 0
    ul
      justify-content space-between
      display flex
      border-bottom  1px solid #eee
      padding 0 0 35px
      strong
        color #333
        font-size 32px
        margin-bottom 17px
      p
        font-size 24px
        color #666
  .footer
    padding 46px 0 0
    span
      color #666
    strong
      color #333
    p
      display flex
      justify-content space-between
    .m
      margin 30px 0



